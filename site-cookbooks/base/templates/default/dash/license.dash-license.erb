<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple Computer//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Product</key>
	<string>Dash</string>
	<key>Name</key>
	<string><%= node[:base][:dash][:license][:name] %></string>
	<key>Email</key>
	<string><%= node[:base][:dash][:license][:email] %></string>
	<key>Licenses</key>
	<string>1</string>
	<key>Timestamp</key>
	<string><%= node[:base][:dash][:license][:timestamp] %></string>
	<key>AppleOrderID</key>
	<string><%= node[:base][:dash][:license][:apple_order_id] %></string>
	<key>Version</key>
	<string><%= node[:base][:dash][:license][:version] %></string>
	<key>Signature</key>
	<data><%= node[:base][:dash][:license][:signature] %></data>
</dict>
</plist>
